{
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Nord",
    "editor.fontFamily": "monospace",
    "editor.matchBrackets": "always",
    "editor.formatOnPaste": true,
    "latex-workshop.latex.outDir": "%DIR%/latex.out",
    "latex-workshop.synctex.afterBuild.enabled": true,
    "latex-workshop.view.pdf.external.synctex.args": [
        "%LINE%",
        "%PDF%",
        "%TEX%"
    ],
    "latex-workshop.view.pdf.external.viewer.args": [
        "%PDF%"
    ],
    "latex-workshop.view.pdf.external.args": {
        "command": "zathura",
        "args": [
            "%PDF%"
        ]
    },
    "latex-workshop.view.pdf.external.synctex": {
        "command": "zathura",
        "args": [
            "--synctex-forward=%LINE%:0:%TEX%",
            "%PDF%"
        ]
    },
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-pdf",
                "-bibtex",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-outdir=%DIR%/latex.out",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "latexmk xepdf",
            "command": "latexmk",
            "args": [
                "-pdfxe",
                "-bibtex",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-outdir=%DIR%/latex.out",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "latexmk luapdf",
            "command": "latexmk",
            "args": [
                "-pdflua",
                "-bibtex",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-outdir=%DIR%/latex.out",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-file-line-error",
                "-synctex=1",
                "-outdir=%DIR%/latex.out",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "biber",
            "command": "biber",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "tectonic",
            "command": "tectonic",
            "args": [
                "%DOC_EXT%",
                "--synctex"
            ]
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk ðŸ”ƒ",
            "tools": [
                "latexmk"
            ]
        },
        {
            "name": "latexmk xelatex",
            "tools": [
                "latexmk xepdf"
            ]
        },
        {
            "name": "latexmk lualatex",
            "tools": [
                "latexmk luapdf"
            ]
        },
        {
            "name": "pdflatex âžž biblatex âžž pdflatex`Ã—2",
            "tools": [
                "pdflatex",
                "bibtex",
                "pdflatex",
                "pdflatex"
            ]
        },
        {
            "name": "pdflatex",
            "tools": [
                "pdflatex"
            ]
        },
        {
            "name": "tectonic",
            "tools": [
                "tectonic"
            ]
        }
    ],
