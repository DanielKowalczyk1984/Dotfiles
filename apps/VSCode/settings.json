{
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Nord",
    "editor.fontFamily": "FuraCode Nerd Font",
    "editor.matchBrackets": "always",
    "editor.formatOnPaste": true,
    "C_Cpp.updateChannel": "Insiders",
    "latex-workshop.latex.outDir": "%DIR%/latex.out",
    "latex-workshop.synctex.afterBuild.enabled": true,
    "latex-workshop.view.pdf.external.args": {
        "command": "zathura",
        "args": [
            "--synctex-editor-command",
            "code --reuse-window -g \"%{input}:%{line}\"",
            "%PDF%"
        ]
    },
    "latex-workshop.view.pdf.external.synctex": {
        "command": "zathura",
        "args": [
            "--synctex-forward=%LINE%:0:%TEX%",
            "%PDF%"
        ]
    },
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-pdf",
                "-bibtex",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-outdir=%DIR%/latex.out",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "latexmk xepdf",
            "command": "latexmk",
            "args": [
                "-pdfxe",
                "-bibtex",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-outdir=%DIR%/latex.out",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "latexmk luapdf",
            "command": "latexmk",
            "args": [
                "-pdflua",
                "-bibtex",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-outdir=%DIR%/latex.out",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-file-line-error",
                "-synctex=1",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "biber",
            "command": "biber",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk ðŸ”ƒ",
            "tools": [
                "latexmk"
            ]
        },
        {
            "name": "latexmk xelatex",
            "tools": [
                "latexmk xepdf"
            ]
        },
        {
            "name": "latexmk lualatex",
            "tools": [
                "latexmk luapdf"
            ]
        },
        {
            "name": "pdflatex âžž biblatex âžž pdflatex`Ã—2",
            "tools": [
                "pdflatex",
                "bibtex",
                "pdflatex",
                "pdflatex"
            ]
        },
        {
            "name": "pdflatex",
            "tools": [
                "pdflatex"
            ]
        }
    ],
    // vim settings
    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false,
        "<C-w>": false,
        "<C-b>": false,
        "<C-x>": false,
        "<C-k>": false,
        "<C-t>": false
    },
    "astyle.astylerc": "/home/daniel/.astylerc",
    "diffEditor.ignoreTrimWhitespace": false,
    "files.exclude": {
        "**/.ccls-cache": true,
        "**/*.aux": true,
        "**/*.bbl": true,
        "**/*.blg": true,
        "**/*.fdb_latexmk": true,
        "**/*.fls": true,
        "**/*.log": true,
        "**/*.xdv": true
    },
    "terminal.external.linuxExec": "alacritty",
    "ccls.completion.enableSnippetInsertion": true,
    "ccls.completion.detailedLabel": true,
    "terminal.explorerKind": "external",
    "files.associations": {
        "*.ipynb": "jupyter"
    },
    "python.pythonPath": "/bin/python",
    "cmake.preferredGenerators": [
        "Unix Makefiles",
        "Ninja"
    ],
    "cSpell.userWords": [
        "ATIF",
        "BDDs",
        "Baptiste",
        "Dantzig",
        "GUBs",
        "Gurobi",
        "Hoeve",
        "MINJOB",
        "Sadykov",
        "Tikzstyle",
        "WSPT",
        "Xeon",
        "ZDDs",
        "bioinformatics",
        "coeffs",
        "convexification",
        "discretization",
        "footnoteref",
        "gchar",
        "integrality",
        "natbib",
        "polytope",
        "pred",
        "preprocess"
    ],
    "latex-workshop.message.update.show": false,
    "latex-workshop.view.pdf.viewer": "external",
    "latex-workshop.chktex.enabled": true,
    "cmake.configureOnOpen": false,
    "C_Cpp.default.cStandard": "c99",
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "window.zoomLevel": 0,
    "terminal.integrated.rendererType": "dom",
    "cSpell.ignoreRegExpList": [
        "GRB.*"
    ],
    "languageTool.language": "en-GB",
    "ltex.language": "en-GB",
    "rust-client.updateOnStartup": true,
    "rust.all_features": true,
    "rust.clippy_preference": "on",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "sync.gist": "ddb9afbd9e2373dcef4c6d7b4e6a38ff",
    "C_Cpp.clang_format_fallbackStyle": "Chromium",
    "C_Cpp.clang_format_sortIncludes": true,
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "[c]": {
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "editor.largeFileOptimizations": false,
    "python.autoComplete.addBrackets": true,
    "python.dataScience.alwaysScrollOnNewCell": true,
    "python.dataScience.ignoreVscodeTheme": true,
    "python.dataScience.themeMatplotlibPlots": true,
    "python.dataScience.sendSelectionToInteractiveWindow": true,
    "python.dataScience.interactiveWindowMode": "perFile",
    "extensions.ignoreRecommendations": false,
    "vs-kubernetes": {
        "vs-kubernetes.helm-path.linux": "/home/danielk/.vs-kubernetes/tools/helm/linux-amd64/helm",
        "vs-kubernetes.draft-path.linux": "/home/danielk/.vs-kubernetes/tools/draft/linux-amd64/draft",
        "vs-kubernetes.minikube-path.linux": "/home/danielk/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
        "vs-kubernetes.kubectl-path.linux": "/home/danielk/.vs-kubernetes/tools/kubectl/kubectl"
    },
    "cSpell.enableFiletypes": [
        "cmake"
    ],
    "editor.multiCursorModifier": "ctrlCmd",
    "fenix.repos": [
        "https://raw.githubusercontent.com/FenixTemplates/Default/master/fenix.json"
    ],
    "fenix.runCommands": "ask",
    "python.languageServer": "Pylance",
    "python.analysis.completeFunctionParens": true
}
